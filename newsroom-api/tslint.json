{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "no-console": false,
        "only-arrow-functions": false,
        "interface-name": false,
        "prefer-for-of": false
    },
    "linterOptions": {
        "exclude": [
            "src/migration/*"
        ]
    }
}