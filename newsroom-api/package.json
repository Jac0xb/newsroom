{
    "name": "newsroom-api",
    "version": "0.0.1",
    "description": "Newsroom Document API",
    "main": "dist/index.js",
    "scripts": {
        "prebuild": "tslint -c tslint.json -p tsconfig.json --fix && swaggerGen -c swagger.config.json",
        "build": "tsc",
        "prestart": "yarn build",
        "start": "node .",
        "start:watch": "nodemon -e ts --watch src --exec 'yarn start'",
        "test": "ts-mocha -p tsconfig.json src/**/*.spec.ts",
        "typeorm": "tsc && ts-node node_modules/typeorm/cli.js"
    },
    "repository": {
        "type": "git",
        "url": "https://capstone-cs.eng.utah.edu/Newsroom/newsroom"
    },
    "keywords": [
        "newsroom",
        "documents",
        "workflows"
    ],
    "author": "Newsroom",
    "license": "UNLICENSED",
    "dependencies": {
        "Blob": "^0.10.0",
        "blob": "0.0.5",
        "cookie-session": "^1.3.3",
        "dotenv": "^7.0.0",
        "express": "^4.16.4",
        "express-session": "^1.16.2",
        "passport": "^0.4.0",
        "passport-google-oauth": "^2.0.0",
        "reflect-metadata": "^0.1.12",
        "swagger-ui-express": "^4.0.2",
        "typedi": "^0.8.0",
        "typeorm": "^0.2.15",
        "typeorm-typedi-extensions": "^0.2.3",
        "typescript-rest": "^2.0.0"
    },
    "devDependencies": {
        "@types/cookie-session": "^2.0.37",
        "@types/dotenv": "^6.1.1",
        "@types/expect": "^1.20.4",
        "@types/express": "^4.16.1",
        "@types/express-session": "^1.15.14",
        "@types/mocha": "^5.2.6",
        "@types/node": "^11.13.0",
        "@types/passport-google-oauth": "^1.0.41",
        "@types/swagger-ui-express": "^3.0.0",
        "mocha": "^6.1.1",
        "nodemon": "^1.18.11",
        "sqlite3": "^4.0.6",
        "ts-mocha": "^6.0.0",
        "ts-node": "^8.2.0",
        "tslint": "^5.15.0",
        "typescript": "^3.4.1",
        "typescript-rest-swagger": "^0.0.24"
    }
}
